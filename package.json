{
  "scripts": {
    "dev": "concurrently \"tsc -w -p tsconfig.dev.json\" \"nodemon -q dist/index.js\"",
    "tools": "concurrently \"jest --watch\" \"esw . --ext .ts --color --watch\" \"npm run prettier-watch\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "esw . --ext .ts --color --watch",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-watch": "^8.0.0",
    "jest": "^29.4.1",
    "mockingoose": "^2.16.2",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.0",
    "joi": "^17.7.0",
    "mongoose": "^6.8.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  }
}
